select model, speed, hd /Выбрать что-то/
from pc /из/
where price < 500 /где цена/
select distinct maker  /выбрать только уникальные строки /
where type = 'printer'

where price > 1000

where color = 'y'

WHERE (CD='12X' OR CD='24X') AND
PRICE < 600

SELECT DISTINCT MAKER, SPEED 
FROM LAPTOP, PRODUCT
WHERE (LAPTOP.MODEL = PRODUCT.MODEL) AND
HD >= 10


select type, model
from product
where maker = 'B'


